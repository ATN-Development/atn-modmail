{
	"arrowParens": "always",
	"cursorOffset": -1,
	"embeddedLanguageFormatting": "auto",
	"endOfLine": "lf",
	"filepath": "**/*.ts",
	"insertPragma": false,
	"printWidth": 80,
	"proseWrap": "always",
	"quoteProps": "consistent",
	"semi": true,
	"singleQuote": false,
	"tabWidth": 2,
	"trailingComma": "es5",
	"useTabs": true
}
